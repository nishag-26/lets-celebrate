<h1>organizers in our DB:</h1>

{{#each organizers}}
    <section class="organizer-summary">
        <h2>{{this.title}}</h2>
        <p>
      
            Rating: {{this.rating}} </p> <br />
            <a href="/organizers/{{this._id}}">More details</a>
            
            <a href="/organizers/{{this._id}}/edit">Edit</a>
       

        <form method="POST" action="/organizers/{{_id}}/delete">
            <button>Delete</button>
        </form>

    </section>
{{/each}}